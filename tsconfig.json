{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "removeComments": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
